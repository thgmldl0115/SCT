{
    "manifest_version": 3,
    "name": "텍스트 추출",
    "version": "1.0",
    "description": "Capture Screen OCR",
    "permissions": ["tabs", "activeTab", "scripting"],
    "action": {
        "default_icon" : "icon 96.png",
        "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }




